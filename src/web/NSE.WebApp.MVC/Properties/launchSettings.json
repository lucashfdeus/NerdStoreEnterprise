{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:52608",
      "sslPort": 44303
    }
  },
  "profiles": {
    "IIS - Development": {
      "commandName": "IISExpress",
